{
  "publisher": "micaiguai",
  "name": "vscode-vue-3-snippets",
  "displayName": "Vue 3 Snippets In Vscode",
  "version": "0.0.12",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "description": "vscode-vue-3-snippets description",
  "author": "micaiguai",
  "license": "MIT",
  "funding": "https://github.com/sponsors/micaiguai",
  "homepage": "https://github.com/micaiguai/vscode-vue-3-snippets#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/micaiguai/vscode-vue-3-snippets"
  },
  "bugs": {
    "url": "https://github.com/micaiguai/vscode-vue-3-snippets/issues"
  },
  "keywords": [
    "javascript",
    "snippet",
    "vue"
  ],
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "sponsor": {
    "url": "https://github.com/sponsors/micaiguai"
  },
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*",
    "snippets/*"
  ],
  "engines": {
    "vscode": "^1.103.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [],
    "configuration": {
      "type": "object",
      "title": "vscode-vue-3-snippets",
      "properties": {}
    },
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "html",
        "path": "./snippets/html.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue.json"
      }
    ]
  },
  "scripts": {
    "build": "tsdown",
    "dev": "nr build --watch --sourcemap",
    "prepare": "nr update",
    "update": "vscode-ext-gen --output src/generated/meta.ts",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@antfu/ni": "^25.0.0",
    "@types/node": "^22.17.2",
    "@types/vscode": "^1.103.0",
    "@vscode/vsce": "^3.6.0",
    "bumpp": "^10.2.3",
    "eslint": "^9.33.0",
    "esno": "^4.8.0",
    "pnpm": "^10.14.0",
    "reactive-vscode": "^0.3.1",
    "tsdown": "^0.14.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vitest": "^3.2.4",
    "vscode-ext-gen": "^1.1.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
